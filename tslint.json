{
  "extends": [],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },

  // disable alphabetical ordering of imports

  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": [false]
  }
}
